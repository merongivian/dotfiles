alias gunstage='git reset HEAD'
alias guncommit='git reset HEAD~$1 --soft'
alias gco='git co'
alias gbrs='git br'
alias gnbr='git co -b'
alias gad='git add'
alias gada='git add . && git status'
alias gul='git pull'
alias glol='git lol'
alias gdf='git icdiff'
alias gdc='git diff --cached'
alias gcom='git commit -m'
alias gcom_all='git add . && git commit -m'
alias gam='git commit --amend'
alias gshw='git show'

alias open_unmerged='vim `git diff HEAD --name-only | uniq`'
alias open_merged='vim `git show --pretty="format:" --name-only | uniq`'

alias be='bundle exec'

alias lcat='lolcat'
alias back='cd ..'

alias ssh='./.ssh/fun_ssh $1'
